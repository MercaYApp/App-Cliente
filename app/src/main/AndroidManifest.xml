<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cosw.mercayapp" >
    <uses-permission android:name="android.permission.INTERNET" ></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="cosw.mercayapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="cosw.mercayapp.GetProducts"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="cosw.mercayapp.TestBarCode"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="cosw.mercayapp.GetFacturas"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="cosw.mercayapp.GetCarrito"
            android:label="@string/app_name" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
